{
  "name": "volta",
  "private": true,
  "version": "0.0.1",
  "description": "React UI Library",
  "main": "components/lib-compiled",
  "license": "MIT",
  "scripts": {
    "prestart": "npm install",
    "start": "npm run build; npm run build-app; npm run server",
    "server": "http-server -a localhost -p 8888 -c-1 -o",
    "build": "webpack --config components/webpack.config.js -d",
    "watch": "webpack --config components/webpack.config.js -d --watch",
    "build-app": "webpack --config app/webpack.config.js -d",
    "watch-app": "webpack --config app/webpack.config.js -d --watch",
    "deploy": "NODE_ENV=production webpack --config app/webpack.config.js -p",
    "lint": "eslint components"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.5",
    "classnames": "^2.2.6",
    "css-loader": "^3.3.0",
    "es6-promise": "^4.2.8",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "http-server": "^0.12.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.2",
    "node-sass": "^4.13.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
