{
  "name": "volta",
  "private": true,
  "version": "0.0.1",
  "description": "React UI Library",
  "main": "components/lib-compiled",
  "license": "MIT",
  "scripts": {
    "prestart": "npm install",
    "start": "npm run build; npm run build-app; npm run server",
    "server": "http-server -a localhost -p 8888 -c-1 -o",
    "build": "webpack --config components/webpack.config.js -d",
    "watch": "webpack --config components/webpack.config.js -d --watch",
    "build-app": "webpack --config app/webpack.config.js -d",
    "watch-app": "webpack --config app/webpack.config.js -d --watch",
    "deploy": "NODE_ENV=production webpack --config app/webpack.config.js -p",
    "lint": "eslint components"
  },
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "~6.9",
    "babel-preset-react": "~6.11",
    "babel-preset-stage-2": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^5.1.1",
    "http-server": "^0.11.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^3.13.1",
    "numeral": "^1.5.3",
    "prop-types": "^15.2.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.0.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14"
  }
}
